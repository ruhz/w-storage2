#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/tree-kill@1.2.2/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules:/mnt/c/share/software/nettool/route/node_modules:/mnt/c/share/software/nettool/node_modules:/mnt/c/share/software/node_modules:/mnt/c/share/node_modules:/mnt/c/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/node_modules/tree-kill/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/tree-kill@1.2.2/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules:/mnt/c/share/software/nettool/route/node_modules:/mnt/c/share/software/nettool/node_modules:/mnt/c/share/software/node_modules:/mnt/c/share/node_modules:/mnt/c/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/node_modules/tree-kill/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tree-kill/cli.js" "$@"
else
  exec node  "$basedir/../tree-kill/cli.js" "$@"
fi
