#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/dev-ip@1.0.1/node_modules/dev-ip/lib/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/dev-ip@1.0.1/node_modules/dev-ip/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/dev-ip@1.0.1/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules:/mnt/c/share/software/nettool/route/node_modules:/mnt/c/share/software/nettool/node_modules:/mnt/c/share/software/node_modules:/mnt/c/share/node_modules:/mnt/c/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/browser-sync@3.0.3/node_modules/browser-sync/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/browser-sync@3.0.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/dev-ip@1.0.1/node_modules/dev-ip/lib/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/dev-ip@1.0.1/node_modules/dev-ip/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/dev-ip@1.0.1/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules:/mnt/c/share/software/nettool/route/node_modules:/mnt/c/share/software/nettool/node_modules:/mnt/c/share/software/node_modules:/mnt/c/share/node_modules:/mnt/c/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/browser-sync@3.0.3/node_modules/browser-sync/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/browser-sync@3.0.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../dev-ip@1.0.1/node_modules/dev-ip/lib/dev-ip.js" "$@"
else
  exec node  "$basedir/../../../../../dev-ip@1.0.1/node_modules/dev-ip/lib/dev-ip.js" "$@"
fi
