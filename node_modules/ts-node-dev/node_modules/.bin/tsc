#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/typescript@4.1.6/node_modules/typescript/bin/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/typescript@4.1.6/node_modules/typescript/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/typescript@4.1.6/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules:/mnt/c/share/software/nettool/route/node_modules:/mnt/c/share/software/nettool/node_modules:/mnt/c/share/software/node_modules:/mnt/c/share/node_modules:/mnt/c/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/ts-node-dev@2.0.0_3daea86cb3316a366ceb7a146ce3b374/node_modules/ts-node-dev/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/ts-node-dev@2.0.0_3daea86cb3316a366ceb7a146ce3b374/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/typescript@4.1.6/node_modules/typescript/bin/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/typescript@4.1.6/node_modules/typescript/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/typescript@4.1.6/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules:/mnt/c/share/software/nettool/route/node_modules:/mnt/c/share/software/nettool/node_modules:/mnt/c/share/software/node_modules:/mnt/c/share/node_modules:/mnt/c/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/ts-node-dev@2.0.0_3daea86cb3316a366ceb7a146ce3b374/node_modules/ts-node-dev/node_modules:/mnt/c/share/software/nettool/route/w-storage2/node_modules/.pnpm/ts-node-dev@2.0.0_3daea86cb3316a366ceb7a146ce3b374/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.1.6/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.1.6/node_modules/typescript/bin/tsc" "$@"
fi
